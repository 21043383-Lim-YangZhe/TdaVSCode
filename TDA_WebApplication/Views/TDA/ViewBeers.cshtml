
@model IEnumerable<TDA_WebApplication.Models.Beers>
<br />
<br />
<table class="table">
    <tdead>
        <tr>
            <th>Drug ID</th>
            <th>Drug Class</th>
            <th>Crcl</th>
            <th>Recommendation</th>
        </tr>
    </tdead>
    <tbody>
        @foreach (Beers item in Model)
        {
            <tr>
                <td>@item.DrugID</td>
                <td>@item.DrugClass</td>
                <td>@item.Crcl</td>
                <td>@item.Recommendation</td>
            </tr>
        }
    </tbody>
</table>